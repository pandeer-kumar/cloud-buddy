{
    "name": "cloud-buddy",
    "version": "1.0.0",
    "description": "react app to provide easy way to deal with day to day cloud related things for their apps",
    "scripts": {
        "format": "prettier --write \"src/**/*.{js,jsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
        "dev": "parcel src/index.html",
        "build": "parcel build --no-source-maps src/index.html",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pandeer-kumar/cloud-buddy.git"
    },
    "author": "Abhishek Kumar",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pandeer-kumar/cloud-buddy/issues"
    },
    "homepage": "https://github.com/pandeer-kumar/cloud-buddy#readme",
    "dependencies": {
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-router-dom": "^6.3.0"
    },
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 Edge versions",
        "last 2 Safari versions"
    ],
    "devDependencies": {
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "gh-pages": "^4.0.0",
        "parcel": "^2.6.0",
        "prettier": "^2.6.2",
        "process": "^0.11.10"
    }
}
